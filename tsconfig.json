{
    "compilerOptions": {
        "module": "ES6",
        "target": "ES6",
        "jsx": "react-jsx",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "moduleResolution": "Node",
        "strictNullChecks": true,
        "esModuleInterop": true,
        "types": ["node"],
        "resolveJsonModule": true
        // "traceResolution": true
    },
    "include": ["src/**/*", "src/static/optional.d.ts"],
    // "files": ["globals.d.ts"]
}